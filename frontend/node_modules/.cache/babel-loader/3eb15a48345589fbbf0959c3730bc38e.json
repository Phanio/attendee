data);\r\n      return data;\r\n    });\r\n}\r\n\r\nfunction inviteAttendee (attendee) {\r\n  return fetch(`${SERVER_URL}/attendees`, {\r\n    method: 'POST',\r\n    headers: {\r\n      'Content-Type': 'application/json'\r\n    },\r\n    body: JSON.stringify(attendee)\r\n  });\r\n}\r\n\r\nfunction editAttendee (attendeeId, attendee) {\r\n  return fetch(`${SERVER_URL}/attendees/${attendeeId}`, {\r\n    method: 'PUT',\r\n    headers: {\r\n      'Content-Type': 'application/json'\r\n    },\r\n    body: JSON.stringify({ ...attendee })\r\n  });\r\n}\r\n\r\nfunction deleteAttendee (attendeeId) {\r\n  return fetch(`${SERVER_URL}/attendees/${attendeeId}`, {\r\n    method: 'DELETE'\r\n  }).then(resp => {\r\n    return resp.status;\r\n  });\r\n}\r\n\r\nexport { getAttendees, inviteAttendee, deleteAttendee, editAttendee, login, siginUp, getCurrentUser };\r\n"]}]}